<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="description" content="">
    <meta name="author" content="Administrator">
    @*<link rel="shortcut icon" href="http://amberit.com.bd/amberImages/favicon.ico">*@
    <link rel="shortcut icon" href="~/Content/img/favicon.ico" type="image/ico">

    <title>ERP | @ViewBag.Title</title>

    <!--=== CSS ===-->
    <!-- Bootstrap -->
    <link href="~/Themes/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!-- jQuery UI -->
    <!--<link href="~/Themes/plugins/jquery-ui/jquery-ui-1.10.2.custom.css" rel="stylesheet" type="text/css" />-->
    <!--[if lt IE 9]>
        <link rel="stylesheet" type="text/css" href="~/Themes/plugins/jquery-ui/jquery.ui.1.10.2.ie.css"/>
    <![endif]-->
    <!-- Theme -->
    <link href="~/Themes/assets/css/main.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/responsive.css" rel="stylesheet" type="text/css" />
    <link href="~/Themes/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Themes/assets/css/fontawesome/font-awesome.min.css">
    <!--[if IE 7]>
        <link rel="stylesheet" href="~/Themes/assets/css/fontawesome/font-awesome-ie7.min.css">
    <![endif]-->
    <!--[if IE 8]>
        <link href="~/Themes/assets/css/ie8.css" rel="stylesheet" type="text/css" />
    <![endif]-->
    @*<link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel='stylesheet' type='text/css'>*@
    <link href="~/Content/GoogleFont.css" rel="stylesheet" />
    <!--=== JavaScript ===-->
    <script type="text/javascript" src="~/Themes/assets/js/libs/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/jquery-ui/jquery-ui-1.10.2.custom.min.js"></script>
    <script type="text/javascript" src="~/Themes/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/libs/lodash.compat.min.js"></script>
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="~/Themes/assets/js/libs/html5shiv.js"></script>
    <![endif]-->
    <!-- Smartphone Touch Events -->
    <script type="text/javascript" src="~/Themes/plugins/touchpunch/jquery.ui.touch-punch.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/event.swipe/jquery.event.move.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/event.swipe/jquery.event.swipe.js"></script>
    <!-- General -->
    <script type="text/javascript" src="~/Themes/assets/js/libs/breakpoints.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/respond/respond.min.js"></script> <!-- Polyfill for min/max-width CSS3 Media Queries (only for IE8) -->
    <script type="text/javascript" src="~/Themes/plugins/cookie/jquery.cookie.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/slimscroll/jquery.slimscroll.horizontal.min.js"></script>
    <!-- Page specific plugins -->
    <!-- Charts -->
    <!--[if lt IE 9]>
        <script type="text/javascript" src="~/Themes/plugins/flot/excanvas.min.js"></script>
    <![endif]-->
    <script type="text/javascript" src="~/Themes/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.tooltip.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.resize.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.time.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/flot/jquery.flot.growraf.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/easy-pie-chart/jquery.easy-pie-chart.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/daterangepicker/moment.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/daterangepicker/daterangepicker.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/blockui/jquery.blockUI.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/fullcalendar/fullcalendar.min.js"></script>
    <!-- Noty -->
    <script type="text/javascript" src="~/Themes/plugins/noty/jquery.noty.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/noty/layouts/top.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/noty/themes/default.js"></script>
    <!-- Forms -->
    <script type="text/javascript" src="~/Themes/plugins/uniform/jquery.uniform.min.js"></script>
    <script type="text/javascript" src="~/Themes/plugins/select2/select2.min.js"></script>
    <!-- App -->
    <script type="text/javascript" src="~/Themes/assets/js/app.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/plugins.js"></script>
    <script type="text/javascript" src="~/Themes/assets/js/plugins.form-components.js"></script>
    <script>
        $(document).ready(function () {
            "use strict";
            $(window).on("load, resize", function () {
                var viewportWidth = $(window).width();
                if (viewportWidth < 767) {
                    $("#container").removeClass("sidebar-closed");
                }
            });
            App.init(); // Init layout and core plugins
            Plugins.init(); // Init all plugins
            FormComponents.init(); // Init all form-specific plugins
        });
    </script>
    <!-- Demo JS -->
    <script type="text/javascript" src="~/Themes/assets/js/custom.js"></script>

</head>
<body class="ng-cloak" ng-app="ABS_App_Lay">
    <!-- Header -->
    <header class="header navbar navbar-fixed-top" role="banner">
        <!-- Top Navigation Bar -->
        <div ng-controller="TopNavigationBarCtrl">
            @Html.Partial("~/Views/Shared/_Partial/_TopNavigationBar.cshtml")
        </div>
        <!-- /top navigation bar -->
        <!-- Project Switcher -->
        @Html.Partial("~/Views/Shared/_Partial/_ProjectSwitcher.cshtml")
        <!-- /project-switcher -->
    </header> <!-- /.header -->
    <div id="container" class="fixed-header sidebar-closed">
        <!-- Sidebar -->
        @Html.Partial("~/Views/Shared/_Partial/_Sidebar.cshtml")
        <!-- /Sidebar -->
        <div id="content">
            @RenderBody()
        </div>
    </div>
    <!-- Footer -->
    <div class="footer">
    </div>
    <!-- /Footer -->
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/ScriptsNg/Modules/ABS_App.js"></script>
    @RenderSection("AngularScript", false)
    <script src="~/ScriptsNg/Controllers/LogoutCtrl.js"></script>
    <script src="~/Scripts/toastr.min.js"></script>
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <script src="~/Scripts/CustomToastrMessage.js"></script>
    
    
    <script src="~/Scripts/ngStorage.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Controllers/TopNavigationNotificationCtrl.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Controllers/sideNavCtrl.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Service/sideNavService.js"></script>

    <script src="~/ScriptsNg/Controllers/TopNavigationBarCtrl.js"></script>
    <script src="~/ScriptsNg/Controllers/TopNavigationNotificationCtrl.js"></script>
    <script src="~/ScriptsNg/Controllers/sideNavCtrl.js"></script>
    <script src="~/ScriptsNg/Controllers/DashboardCtrl.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Service/customCodeService.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Service/sideNavService.js"></script>

    <link href="~/Themes/assets/css/CustomStyles.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.signalR-2.2.0.js"></script>
    <script src="~/signalr/hubs"></script>
</body>
</html>