<table class="table  table-hover table-striped">
    <thead class="grid-top-panel">
        <tr>

            <th>SL No:</th>
            <th>Chemical</th>
            <th>Quantity</th>
            <th>Unit</th>

        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListDying">

            <td>{{ $index + 1 }}</td>
            <td>{{dataModel.ItemName}}</td>
            <td><input type="number" min="0" id="txtQty" name="txtQty" class="form-control" ng-model="dataModel.Qty" ng-blur="calculationDyng(dataModel)" /></td>

            <td> {{dataModel.UOMName}} </td>
        </tr>

        <tr> <td></td> <td><span style="font-weight:bold">Total</span></td><td><span style="font-weight:bold">{{dyngTotal}}</span></td> <td></td>  </tr>

    </tbody>
</table>
<p ng-show="(ListDying).length == 0" style="color:red">Nothing found!</p>

<div class="loadListDyingmore">
    <div ng-show="loaderListDyingMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>