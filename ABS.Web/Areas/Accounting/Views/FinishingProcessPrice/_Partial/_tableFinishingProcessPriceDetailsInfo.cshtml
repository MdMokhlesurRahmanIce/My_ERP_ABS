
<table class="table  table-hover table-striped">
    <thead class="grid-top-panel">
        <tr>

            <th>SL No:</th>
            <th>Finishing Process</th>
            <th>Process Price No</th>
            <th>Price</th>
            <th>Price Date</th>
            <th>Currency</th>
            <th>Remarks</th>

            <th>Is Active</th>

        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListFppDetails">

            <td>{{ $index + 1 }}</td>
            <td>{{dataModel.FinishingProcessName}}</td>
            <td>{{dataModel.ProcessPriceNo}}</td>

            <td> {{dataModel.Price}} </td>

            <td> {{dataModel.PriceDate | date:'dd-MM-yyyy'}} </td>
            <td>{{dataModel.CurrencyName}}</td>
            <td>{{dataModel.Remarks}}</td>
            <td><input type="checkbox" ng-model="dataModel.IsActive" disabled="disabled"></td>
        </tr>
    </tbody>
</table>
<p ng-show="(ListFppDetails).length == 0" style="color:red">Nothing found!</p>

<div class="loadmore">
    <div ng-show="loaderMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>