@{
    ViewBag.Title = "Pre-Costing";
}
<div ng-controller="tokenCtrl">
    <div class="container" ng-controller="preCostingCtrl">
        <!-- Breadcrumbs line -->
        <div ng-hide="pageValue" class="ng-hide">
            <!--Logged User-->
            <input type="hidden" id="hUserID" name="userID" value="@Session["UserID"].ToString()" ng-model="UserID" />
            <!--Logged Company-->
            <input type="hidden" id="hCompanyID" name="companyID" value="@Session["CompanyID"].ToString()" ng-model="CompanyID" />
        </div>

        <!-- Breadcrumbs line -->
        <div ng-controller="BreadcrumbCtrl">
            @Html.Partial("~/Areas/SystemCommon/Views/Shared/_Partial/_Breadcrumbs.cshtml")
        </div>
        <!-- /Breadcrumbs line -->
        <!--=== Page Header ===-->
        <div class="page-header">

        </div>


        <div class="row">
            <div class="col-md-12">
                @Html.Partial("~/Areas/SystemCommon/Views/DeleteModal/_modalCmnDelete.cshtml")
            </div>
        </div>


        <!-- /Page Header -->
        <!--=== Page Content ===-->

        <div class="row">

            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{PageTitle}}</h4>
                        <div class="toolbar no-padding">

                            <div class="btn-group" @*style="margin-left:360px;"*@>

                                <span id="reset" class="btn btn-danger margin-right-btn" ng-click="clear()">
                                    <i class="icon-refresh"></i> Reset
                                </span>

                                <span id="Show" class="btn btn-info margin-right-btn" ng-click="ShowHide()">
                                    <i class="icon-search" ng-show="IsListIcon"></i> <i ng-show="IsCreateIcon" class="icon-plus-sign"></i> {{btnShowText}}
                                </span>

                                <span id="save" class="btn btn-success margin-right-btn" ng-click="save()" ng-disabled="frmPreCosting.$invalid">
                                    <i class="icon-save"></i> {{btnSaveText}}
                                </span>


                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>

                        </div>
                    </div>
                    @*<div class="widget-content" ng-show="IsShow">
                            @Html.Partial("~/Areas/Accounting/Views/BillOfMaterial/_Partial/_formBillOfMaterial.cshtml")
                        </div>*@
                    <div class="widget-content">
                        <div class="tabbable box-tabs">
                            <ul class="nav nav-tabs" style="margin-right:350px">

                                <li><a href="#box_tab6" data-toggle="tab" ng-click="LoadTotalGrd()"><i class="icon-briefcase"></i> Total</a></li>
                                <li><a href="#box_tab5" data-toggle="tab" ng-click="LoadYarnGrd()"><i class="icon-briefcase"></i> Yarn</a></li>
                                <li><a href="#box_tab3" data-toggle="tab" ng-click="LoadFinishingGrd()"><i class=" icon-briefcase"></i> Finishing</a></li>
                                <li><a href="#box_tab4" data-toggle="tab" ng-click="LoadSizingGrd()"><i class=" icon-briefcase"></i> Sizing</a></li>

                                <li><a href="#box_tab2" data-toggle="tab" ng-click="LoadDyingGrd()"><i class=" icon-briefcase"></i> Dying </a></li>
                                @*<li class="active"><a href="#box_tab1" data-toggle="tab"><i class="icon-briefcase"></i> Bill Of Material Info</a></li>*@

                            </ul>
                            <div class="tab-content">

                                <div class="row" ng-show="IsShow">
                                    <div class="col-md-12">

                                        @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_formPreCosting.cshtml")


                                    </div>
                                </div>
                                <div>
                                    <br />
                                    <br />
                                </div>
                                <div class="tab-pane" id="box_tab2">

                                    <div class="row" ng-hide="IsHiddenDying">
                                        <div class="col-md-12">
                                            <div class="widget box">
                                                <div class="widget-header">
                                                    <h4><i class="icon-reorder"></i> {{TitleDyingGrid}}</h4>
                                                    <div class="toolbar no-padding">
                                                        <div class="btn-group">
                                                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="widget-content no-padding">
                                                    @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tableDying.cshtml")
                                                </div>
                                            </div>
                                        </div> <!-- /.col-md-12 -->
                                    </div>
                                    @*//*@

                                </div>

                                <div class="tab-pane" id="box_tab3">
                                    <div class="row" ng-hide="IsHiddenFinishing">
                                        <div class="col-md-12">
                                            <div class="widget box">
                                                <div class="widget-header">
                                                    <h4><i class="icon-reorder"></i> {{TitleFinishingGrid}}</h4>
                                                    <div class="toolbar no-padding">
                                                        <div class="btn-group">
                                                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="widget-content no-padding">
                                                    @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tableFinishing.cshtml")

                                                </div>
                                            </div>
                                        </div> <!-- /.col-md-12 -->
                                    </div>
                                    @*//*@
                                </div>
                                <div class="tab-pane" id="box_tab4">
                                    <div class="row" ng-hide="IsHiddenSizing">
                                        <div class="col-md-12">
                                            <div class="widget box">
                                                <div class="widget-header">
                                                    <h4><i class="icon-reorder"></i> {{TitleSizingGrid}}</h4>
                                                    <div class="toolbar no-padding">
                                                        <div class="btn-group">
                                                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="widget-content no-padding">
                                                    @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tableSizing.cshtml")

                                                </div>
                                            </div>
                                        </div> <!-- /.col-md-12 -->
                                    </div>
                                    @*//*@


                                    @*ng-hide="IsHiddenDetail"*@

                                    @*<div class="row" >
                                            <div class="col-md-12">
                                                <div class="widget box">
                                                    <div class="widget-header">
                                                        <h4><i class="icon-reorder"></i> {{ListTitleAdjustmentDeatails}}</h4>
                                                        <div class="toolbar no-padding">
                                                            <div class="btn-group">
                                                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="widget-content no-padding">

                                                        @Html.Partial("~/Areas/Sales/Views/PartyPaymentMethod/_Partial/_tableAdjustmentDetail.cshtml")

                                                    </div>
                                                </div>
                                            </div>  /.col-md-12
                                        </div>*@
                                </div>


                                <div class="tab-pane" id="box_tab5">
                                    <div class="row" ng-hide="IsHiddenYarn">
                                        <div class="col-md-12">
                                            <div class="widget box">
                                                <div class="widget-header">
                                                    <h4><i class="icon-reorder"></i> {{TitleYarnGrid}}</h4>
                                                    <div class="toolbar no-padding">
                                                        <div class="btn-group">
                                                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="widget-content no-padding">
                                                    @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tableYarn.cshtml")

                                                </div>
                                            </div>
                                        </div> <!-- /.col-md-12 -->
                                    </div>
                                    @*//*@
                                </div>
                                <div class="tab-pane" id="box_tab6">
                                    <div class="row" ng-hide="IsHiddenTotal">
                                        <div class="col-md-12">
                                            <div class="widget box">
                                                <div class="widget-header">
                                                    <h4><i class="icon-reorder"></i> {{TitleTotalGrid}}</h4>
                                                    <div class="toolbar no-padding">
                                                        <div class="btn-group">
                                                            <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="widget-content no-padding">
                                                    @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tableTotal.cshtml")

                                                </div>
                                            </div>
                                        </div> <!-- /.col-md-12 -->
                                    </div>
                                  
                                </div>

                                @*<div class="tab-pane" id="box_tab5">

                                                        @Html.Partial("~/Areas/Accounting/Views/BillOfMaterial/_Partial/_tableYarn.cshtml")

                                    </div>*@

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /.col-md-12 -->


        <div class="row" ng-hide="IsHiddenDetail">
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{ListTitleDeatails}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="widget-content no-padding">
                        @*@Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tablePreCostingDetailsInfo.cshtml")*@
                    </div>
                </div>
            </div> <!-- /.col-md-12 -->
        </div>



        <div class="row" ng-hide="IsHidden">
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{ListTitleMasters}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="widget-content no-padding">

                        @Html.Partial("~/Areas/Accounting/Views/PreCosting/_Partial/_tablePreCostingMastrsInfo.cshtml")

                    </div>
                </div>
                <div class="clearfix"></div>
            </div> <!-- /.col-md-12 -->
        </div>
         
        <!-- /Page Content -->


    </div>
</div>
<!-- /.container -->
@section AngularScript{


    <script src="~/Areas/Accounting/ScriptsNg/Controllers/PreCostingCtrl.js"></script>
    <script src="~/Areas/Accounting/ScriptsNg/Service/PreCostingService.js"></script> 
    <script src="~/Areas/Inventory/ScriptsNg/Directives/Conversion.js"></script>
    <script src="~/Scripts/CustomToastrMessage.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Controllers/BreadcrumbCtrl.js"></script>
    <script src="~/Areas/Token/TokenController.js"></script>

}
