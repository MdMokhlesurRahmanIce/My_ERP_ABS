<table class="table  table-hover table-striped">
    <thead class="grid-top-panel">
        <tr>

            <th>SL No:</th>
            <th>Process Code</th>
            <th>Process Full Name</th>
            <th>Unit Price</th>

        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListFinishing">

            <td>{{ $index + 1 }}</td>
            <td>{{dataModel.ProcessCode}}</td>
            <td> {{dataModel.ProcessFullName}} </td> 
            <td><input type="number" min="0" id="txtUnitPrice" name="txtUnitPrice" class="form-control" ng-model="dataModel.UnitPrice" ng-blur="calculationFinish(dataModel)" /></td>

        </tr>

        <tr> <td></td> <td><span style="font-weight:bold">Total</span></td> <td></td> <td><span style="font-weight:bold">{{finishTotal}}</span></td> </tr>

    </tbody>
</table>
<p ng-show="(ListFinishing).length == 0" style="color:red">Nothing found!</p>

<div class="loadListFinishingmore">
    <div ng-show="loaderListFinishingMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>