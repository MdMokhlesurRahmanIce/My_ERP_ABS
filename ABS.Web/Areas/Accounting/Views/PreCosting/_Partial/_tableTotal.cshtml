<table class="table  table-hover table-striped">
    <thead class="grid-top-panel">
        <tr>

            <th>Yarn</th>
            <th>Dying</th>
            <th>Sizing</th>
            <th>Finishing</th>
            <th>Over Head</th>
            <th>Total</th>

             @*UnitPrice,*@ 
      
          </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListTotal">
            
            <td>{{dataModel.YarnCost}}</td> 
            <td>{{dataModel.DyingCost}}</td>
            <td>{{dataModel.SizeCost}}</td>
            <td>{{dataModel.FinishingCost}}</td> 
            @*<td>{{dataModel.OverHeadCost}}</td>*@ 
            <td><input type="number" min="0" id="txtOverHeadCost" name="txtOverHeadCost" class="form-control" ng-model="dataModel.OverHeadCost" ng-blur="calculationOverHead(dataModel)" /></td>

            <td>{{dataModel.ttlTotal}} </td>
        </tr>
         

    </tbody>
</table>
<p ng-show="(ListTotal).length == 0" style="color:red">Nothing found!</p>

<div class="loadTotalmore">
    <div ng-show="loaderTotalMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>