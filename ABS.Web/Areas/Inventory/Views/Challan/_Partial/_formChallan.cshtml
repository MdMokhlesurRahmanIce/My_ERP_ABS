                <div class="form-group">
                    <label class="col-md-4 control-label" for="input17">Transaction Type </label>
                    <div class="col-md-6">
                        <select id="ddlChallanType" class="select2-select-00"
                                ng-model="lstChallanTypeList"
                                name="ddlChallanType"
                                ng-change="LoadByChallanTransType()"
