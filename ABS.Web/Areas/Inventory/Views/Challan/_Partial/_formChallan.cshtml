
<form novalidate name="frmChallan" id="challan" class="form-horizontal row-border">
    <input type="hidden" id="challanID" readonly="readonly" ng-model="CHID" />
    <input type="hidden" id="challanTypeID" readonly="readonly" ng-model="ChallanTypeID" />
    <input type="hidden" id="menuID" readonly="readonly" ng-model="MenuID" />


        <div class="row">
            <div class="col-md-6">

                <div class="form-group">
                    <label class="col-md-4 control-label">GRR No </label>
                    <div class="col-md-6">
                        <input type="text"
                               id="txtChallanNo"
                               class="form-control"
                               name="txChallanNo" ng-model="HCHNo" placeholder="New" ng-readonly="true" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">Ref Challan No </label>
                    <div class="col-md-6">
                        <input type="text"
                               id="txtRefChallanNo"
                               class="form-control"
                               name="txtRefChallanNo" ng-model="RefChallanNo" placeholder="Ref Challan No" required />
                    </div>
                    <span class="error" ng-show="(frmChallan.$dirty||submitted) && frmChallan.txtRefChallanNo.$error.required">*</span>
                </div>



