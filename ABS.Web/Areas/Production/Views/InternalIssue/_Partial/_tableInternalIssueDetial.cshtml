<input id="hdoDetailSearch" ng-model="HdoDetailSearch" type="text" class="form-control search" placeholder="Search...">

<table class="table  table-hover general-table">
    <thead>
        <tr>
            <th style="display:none">Issue No</th>
            <th style="display:none">Set ID</th>

            <th>SL.</th>
            <th>Issue NO.</th>
            <th>Set NO.</th>
            <th>Artical</th>           
            <th>Ball Issue Date</th>
            <th>Is Issued Ball</th>
            <th>Dyning Received Date</th>
            <th>Is Received Dying</th>

            <th>Dyning Issue Date</th>
            <th>Is Issued Dyning</th>

            <th>LCB Received  Received</th>
            <th>Is Received  LCB</th>
            
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in InternalIssueDetails">
            <td style="display:none">{{dataModel.IssueID}}</td>
            <td style="display:none">{{dataModel.SetID}}</td>
            <td>{{$index+1}}</td>
            <td>{{dataModel.IssueNo}}</td>
            <td>{{dataModel.SetNo}}</td>
            <td>{{dataModel.ArticalNo}}</td>
            <td>{{dataModel.IssBallDate}}</td>
            <td>{{dataModel.IsIssuedBall}}</td>
            <td>{{dataModel.ReceivedDyDate}}</td>
            <td>{{dataModel.IsReceivedDy}}</td>
            <td>{{dataModel.IssDyDate}}</td>
            <td>{{dataModel.IsIssuedDy}}</td>

            <td>{{dataModel.ReceivedLCBDate}}</td>
            <td>{{dataModel.IsReceivedLCB}}</td>

            <td style="text-align:right; color:white">
                
                <span class="label label-danger label-mini">
                    <a href="#" class="bs-tooltip" title="Select" ng-click="GetInternalIssueByIssuID(dataModel)">
                        <i class="icon-check"></i>
                    </a>
                </span>

            </td>
        </tr>
    </tbody>
</table>
<p ng-show="(ListPIDetails).length == 0" style="color:red">Nothing found!</p>

<div class="loadmore">
    <div ng-show="loaderMore" ng-class="result">
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>


