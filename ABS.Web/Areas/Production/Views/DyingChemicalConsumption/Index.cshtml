@{
    ViewBag.Title = "Chemical Process";
}
<div ng-controller="tokenCtrl">
    <div class="container" ng-controller="DyingChemicalConsumptionCtrl">
        <!-- /Breadcrumbs line -->
        <div ng-controller="BreadcrumbCtrl">
            @Html.Partial("~/Areas/SystemCommon/Views/Shared/_Partial/_Breadcrumbs.cshtml")
        </div>
        <!-- /Breadcrumbs line -->
        <!--=== Page Header ===-->
        <div class="page-header">

        </div>
        <!-- /Page Header -->
        <!--=== Page Content ===-->
        <!--==Delete Modal From System Common==-->
        <div class="row">
            <div class="col-md-12">
                @Html.Partial("~/Areas/SystemCommon/Views/DeleteModal/_modalCmnDelete.cshtml")
            </div>
        </div>

        <!--==Delete Modal From System Common==-->
        <div class="row">
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{PageTitle}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                @Html.Partial("~/Areas/SystemCommon/Views/CommonButton/_cmnButton.cshtml")
                                @*<span id="reset" class="btn btn-danger margin-right-btn" ng-click="clear()"><i class="icon-refresh"></i> Reset</span>
                                    <span id="Show" class="btn btn-info margin-right-btn" ng-click="ShowHide()"><i id="Icon-Show" class="icon-search"></i>{{btnShowList}}</span>
                                    <span id="save" class="btn btn-success margin-right-btn" ng-click="SaveUpdateBallMRR()" ng-if="UserCommonEntity.EnableInsert" ng-show="IsShowSave" ng-disabled="IsSaveDisable">
                                        <i id="Icon-Save" class="icon-save"></i> {{btnSaveText}}
                                    </span>
                                    <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>*@
                            </div>
                        </div>
                    </div>
                    <div id="frmEntry" class="widget-content">
                        @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_formDyingChemicalConsumption.cshtml")@*Entry*@
                    </div>
                </div>
            </div> <!-- /.col-md-12 -->
        </div>
        <div class="row" id="SetNoSetupPopup">
            <div class="col-md-12">
                @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_formSetNoSetup.cshtml")
            </div> <!-- /.col-md-12 -->
        </div> <!-- /.row -->

        <div class="row" ng-show="IsShowDetailMachine">
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{MachineSetupTitle}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="widget-content no-padding">
                        @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_tableDyingChemicalConsumptionMachineSetupDetail.cshtml")@*Machine Load on Item change*@
                        @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_tableDyingChemicalConsumptionOperationDetails.cshtml")@*Operation Load on Item change*@
                    </div>
                </div>
            </div> <!-- /.col-md-12 -->
        </div>
        <!-- MRR DETAILS Begin-->
        <div id="frmDetail" class="row">
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{ListDetail}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                <input type="button" id="add" class="btn btn-info margin-right-btn" value="Add New Rows" ng-click="AddMrrDetails()" ng-disabled="frmPrdDyingMRRMaster.$invalid" />
                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="widget-content no-padding" ng-show="IsShow">
                        @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_tableDyingChemicalConsumptionDetails.cshtml")
                    </div>
                </div>
            </div> <!-- /.col-md-12 -->
        </div>
        <!-- MRR DETAILS END-->
        <!--MRR Get All-->
        <div id="frmList" class="row">
            @*ng-hide="IsHidden"*@
            <div class="col-md-12">
                <div class="widget box">
                    <div class="widget-header">
                        <h4><i class="icon-reorder"></i> {{ListMaster}}</h4>
                        <div class="toolbar no-padding">
                            <div class="btn-group">
                                <span class="btn btn-xs widget-collapse"><i class="icon-angle-down"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="widget-content no-padding">
                        @Html.Partial("~/Areas/Production/Views/DyingChemicalConsumption/_Partial/_tableDyingChemicalConsumption.cshtml")@*Master List*@
                    </div>
                </div>
            </div>
        </div> <!-- /.col-md-12 -->
        <!--MRR Item Modal-->
        <div class="row">
            @Html.Partial("~/Areas/SystemCommon/Views/PublicPartialViews/_ProductionItemModal.cshtml")
        </div>
    </div>
    <!--MRR Get All ENd-->
    <!-- /Page Content -->
</div>
@*</div>*@
<!-- /.container -->
@section AngularScript{
    <script src="~/Areas/Production/ScriptsNg/Directives/DropdownMultiselect.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Controllers/DyingChemicalConsumptionCtrl.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Service/DyingChemicalConsumptionService.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Service/commonComboBoxGetDataService.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Service/CrudService.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Directives/Conversion.js"></script>
    <script src="~/Areas/Production/ScriptsNg/Directives/Scroller.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Controllers/BreadcrumbCtrl.js"></script>
    <script src="~/Areas/Token/TokenController.js"></script>
    <!-- / Item Master Modal Refference -->
    <script src="~/Areas/SystemCommon/ScriptsNg/Service/PublicService.js"></script>
    <script src="~/Areas/SystemCommon/ScriptsNg/Directives/AttrEnter.js"></script>

}
