@*<input id="custSearch" ng-model="custSearch" type="text" class="form-control search" placeholder="Search...">*@
<table class="table table-striped table-bordered table-hover dataTable">
    <thead>
        <tr>
            <th class="text-center col-md-1">SL.</th>
            <th class="text-center col-md-3">Article </th>
            <th class="text-center col-md-2">Process</th>
            <th class="text-center col-md-2">Operation</th>
            <th class="text-center col-md-2">Item</th>
            <th class="text-center col-md-2">MinQty</th>
            <th class="text-center col-md-2">MaxQty</th>
            <th class="text-center col-md-2">Unit</th>
            <th style="text-align:right;">Options</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListDyingOperationSetup | orderBy:'IsDeleted' " ng-show="!dataModel.IsDeleted">
            <td class="text-center col-md-1">{{$index+1}}</td>
            <td class="text-center col-md-3">{{dataModel.ArticleNo}}</td>
            <td class="text-center col-md-2">{{dataModel.ProcessName}}</td>
            <td class="text-center col-md-2">{{dataModel.OperationName}}</td>
            <td class="text-center col-md-2">{{dataModel.ChemicalArticleNo}}</td>
            <td class="text-center col-md-2">{{dataModel.MinQty}}</td>
            <td class="text-center col-md-2">{{dataModel.MaxQty}}</td>
            <td class="text-center col-md-2">{{dataModel.UOMName}}</td>
            <td style="text-align:right; color:white">
                <span class="label label-danger label-mini">
                    <a href="javascript:void(0);" class="bs-tooltip" title="Delete" ng-click="deleteDetailsList(dataModel)">
                        <i class="icon-trash"></i>
                    </a>
                </span>
            </td>
        </tr>
    </tbody>
</table>
@*| filter:custSearch*@
