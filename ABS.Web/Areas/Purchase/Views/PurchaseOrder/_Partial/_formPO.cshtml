                    <input type="text"
                           id="txtFoundNo"
                           class="form-control"
                           name="txtFoundNo" ng-model="FoundNo" placeholder="Found No" required />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.txtFoundNo.$error.required">*</span>
            </div>*@




            <div class="form-group">
                <label class="col-md-5 control-label">Order Type </label>
                <div class="col-md-6">

                    <select id="ddlOrderType" class="select2-select-00"
                            ng-model="OrderType"
                            name="ddlOrderType"
                            data-placeholder="--Select Order Type--"
                            style="width: 100%" ng-options="ord.ComboID as ord.ComboName for ord in listOrderType">
                        <option></option>
                    </select>
                </div>
            </div>


            <div class="form-group">
                <label class="col-md-5 control-label">Money Tr. Type </label>
                <div class="col-md-6">

                    <select id="ddlMoneyTrnsType" class="select2-select-00"
                            ng-model="MoneyTrnsTypes"
                            name="ddlMoneyTrnsType"
                            data-placeholder="--Select Money Trns. Type--"
                            style="width: 100%" ng-options="mtt.ComboID as mtt.ComboName for mtt in listMoneyTrnsType">
                        <option></option>
                    </select>
                </div>
            </div>


            <div class="form-group">
                <label class="col-md-5 control-label">LC Voucher No </label>
                <div class="col-md-6">
                    <input type="text"
                           id="txtLCBankVoucherNo"
                           class="form-control"
                           name="txtLCBankVoucherNo" ng-model="LCBankVoucherNo" placeholder="LC Bank Voucher No" required />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.txtLCBankVoucherNo.$error.required">*</span>
            </div>




            <div class="form-group">
                <label class="col-md-5 control-label"> LC Voucher Date </label>
                <div class="col-md-6">
                    <input type="text"
                           id="dtLCVoucherDate"
                           class="form-control datepicker"
                           name="dtLCVoucherDate" ng-model="LCVoucherDate" required @*ng-disabled="true" readonly="readonly"*@ />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.dtLCVoucherDate.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-5 control-label" for="input17">Bank </label>
                <div class="col-md-6">
                    <select id="ddlBankAdvising" class="select2-select-00"
                            ng-model="lstBankAdvisingList"
                            name="ddlBankAdvising"
                            data-placeholder="--Select Bank Advising--"
                            style="width: 100%" ng-change="LoadBranchByBankID()" ng-options="bnk.BankID as bnk.BankName for bnk in listBankAdvising" required>
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.ddlBankAdvising.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-5 control-label" for="input17">Bank Branch </label>
                <div class="col-md-6">
                    <select id="ddlBankBranch" class="select2-select-00"
                            ng-model="lstBankBranchList"
                            name="ddlBankBranch"
                            data-placeholder="--Select Bank Branch--"
                            style="width: 100%" ng-options="bnkBranch.BranchID as bnkBranch.BranchName for bnkBranch in listBankBranch" required>
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.ddlBankBranch.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-5 control-label">Terms&Condition </label>
                <div class="col-md-6">
                    <span class="label label-success label-mini">
                        <a href="javascript:void(0);" ng-href="#HDOMasterModal" data-toggle="modal" class="bs-tooltip" title="Terms&Condition">
                            <i class="glyphicon glyphicon-search" @*ng-click="loadQuotationDetailPop_Records(dataModel)"*@></i>
                        </a>
                    </span>
                </div>
            </div>  

             

        </div>
        <div class="col-md-4">




            <div class="form-group">
                <label class="col-md-4 control-label">Account No</label>
                <div class="col-md-7">
                    <input type="text"
                           id="txtAccountNo"
                           class="form-control"
                           name="txtAccountNo" ng-model="AccountNo"  placeholder="Account No" required />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.txtAccountNo.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-4 control-label">Amount </label>
                <div class="col-md-7">
                    <input type="number"
                           id="txtAmount"
                           class="form-control"
                           name="txtAmount" ng-model="Amount" min="1" placeholder="Amount" required />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.txtAmount.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-4 control-label"> Shipment Date </label>
                <div class="col-md-7">
                    <input type="text"
                           id="dtShipmentDate"
                           class="form-control datepicker"
                           name="dtShipmentDate" ng-model="ShipmentDate" required @*ng-disabled="true" readonly="readonly"*@ />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.dtShipmentDate.$error.required">*</span>
            </div>

            <div class="form-group">
                <label class="col-md-4 control-label"> Expire Date </label>
                <div class="col-md-7">
                    <input type="text"
                           id="dtExpireDate"
                           class="form-control datepicker"
                           name="dtExpireDate" ng-model="ExpireDate" required @*ng-disabled="true" readonly="readonly"*@ />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.dtExpireDate.$error.required">*</span>

            </div>

            <div class="form-group">
                <label class="col-md-4 control-label">Description </label>
                <div class="col-md-7">
                    <input type="text"
                           id="txtDescription"
                           class="form-control"
                           name="txtDescription" ng-model="Description" />
                </div>
            </div>

        </div>
    </div>

   
    <div class="clearfix"></div>

    <div class="row">
        <div class="col-md-12">

            <div class="form-group">
                <label class="col-md-1 control-label" style="width:11%;">Remarks </label>

                <div class="col-md-10">
                    <textarea rows="2" cols="3"
                              id="txtRemarks"
                              class="form-control"
                              name="txtRemarks" ng-model="Remarks" style="width:103.5%;"></textarea>
                </div>
            </div>
        </div>
    </div>




