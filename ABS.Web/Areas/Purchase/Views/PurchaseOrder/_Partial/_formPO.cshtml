
<form novalidate name="frmPO" id="pO" class="form-horizontal row-border">
    <input type="hidden" id="pOID" readonly="readonly" ng-model="POID" /> 

    <div class="row">
        <div class="col-md-4">

            <div class="form-group">
                <label class="col-md-4 control-label">PO No </label>
                <div class="col-md-7">
                    <input type="text"
                           id="txtPONo"
                           class="form-control"
                           name="txPONo" ng-model="HPONo" placeholder="New" ng-readonly="true" />
                </div>
            </div>

            <div class="form-group">
                <label class="col-md-4 control-label">PO Date </label>
                <div class="col-md-7">
                    <input type="text"
                           id="dtPODate"
                           class="form-control datepicker"
                           name="dtPODate" ng-model="PODate" required ng-disabled="true" @*readonly="readonly"*@ />
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.dtPODate.$error.required">*</span>
            </div>
             
            <div class="form-group">
                <label class="col-md-4 control-label">Quotation No </label>
                <div class="col-md-7">

                    <select id="ddlStatementNo" class="select2-select-00"
                            ng-model="StatementNo" 
                            name="ddlStatementNo"
                            ng-change="LoadItemByStatementNoChange()"
                            data-placeholder="--Select Quotation No--"
                            style="width: 100%" ng-options="pn.QuotationID as pn.QuotationNo for pn in listStatementNo">
                        <option></option>
                    </select>
                </div>
            </div>


            <div class="form-group">
                <label class="col-md-4 control-label">SPR No </label>

                <div class="col-md-7">
                    <select id="ddlSPRNo" class="select2-select-00"
                            ng-model="SprNo"
                            name="ddlSPRNo"
                            @*ng-change="LoadItemBySPRNoChange()"*@
                            data-placeholder="--Select SPR No--"
                            style="width: 100%" ng-options="sprn.RequisitionID as sprn.RequisitionNo for sprn in listSprNo" ng-readonly="true" required>
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.ddlSPRNo.$error.required">*</span>
            </div>



            <div class="form-group">
                <label class="col-md-4 control-label" for="input17">Supplier </label>
                <div class="col-md-7">
                    <select id="ddlParty" class="select2-select-00"
                            ng-model="lstPartyList"
                            name="ddlParty"
                            data-placeholder="--Select Supplier--"
                            style="width: 100%" ng-options="buy.UserID as buy.UserFullName for buy in listParty" required ng-readonly="true">
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.ddlParty.$error.required">*</span>
            </div>


            <div class="form-group">
                <label class="col-md-4 control-label" for="input17">Currency </label>
                <div class="col-md-7">
                    <select id="ddlCurrency" class="select2-select-00"
                            ng-model="lstCurrencyList"
                            name="ddlCurrency"
                            data-placeholder="--Select Currency--"
                            style="width: 100%" ng-options="curn.Id as curn.CurrencyName for curn in listCurrency" required ng-readonly="true">
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPO.$dirty||submitted) && frmPO.ddlCurrency.$error.required">*</span>
            </div>

              
            <div class="form-group">
                <label class="col-md-4 control-label" for="input">Attachment</label>
                <div class="col-md-7">
                    <input type="file" id="file" name="file" multiple onchange="angular.element(this).scope().getFileDetails(this)" />
                </div>
            </div>

           



            @*<div class="form-group">
                    <label class="col-md-4 control-label" for="input17"> Item Type  </label>
                    <div class="col-md-6">

                        <select id="ddlItemType" class="select2-select-00"
                                ng-model="ItemType"
                                name="ddlItemType"
                                data-placeholder="--Select Item Type--"
                                style="width: 100%" ng-options="Type.Value as Type.Item for Type in Types" ng-change="getItemGroupsByType()"  >
                            <option></option>
                        </select>
                    </div>

                </div>*@


