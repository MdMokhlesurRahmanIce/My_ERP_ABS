                        <div class="col-md-3">
                            <input type="number"
                                   id="txtOverdueInterest"
                                   class="form-control"
                                   name="txtOverdueInterest" ng-model="OverdueInterest" min="0" required />
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.txtOverdueInterest.$error.required">*</span>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="input17">Status </label>
                        <div class="col-md-4">
                            <select id="ddlPIStatus" class="select2-select-00"
                                    ng-model="lstPIStatusNG"
                                    name="ddlPIStatus"
                                    data-placeholder="--Select Status--"
                                    style="width: 100%" ng-options="vl.ComboID as vl.ComboName for vl in listPIStatus" required>
                                <option></option>
                                @*<option ng-repeat="vl in listValidity" value="{{vl.ComboID}}">{{vl.ComboName}}</option>*@
                            </select>
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlValidity.$error.required">*</span>
                    </div>


