
<form novalidate name="frmPI" id="pI" class="form-horizontal row-border">
    <input type="hidden" id="pIID" readonly="readonly" ng-model="PIID" />
    <input type="hidden" id="pITypeID" readonly="readonly" ng-model="PITypeID" />
    <input type="hidden" id="menuID" readonly="readonly" ng-model="MenuID" />


    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label class="col-md-4 control-label" for="input17">Company </label>
                <div class="col-md-6">
                    @*<p>Selected Company: {{lstCompanyList}}</p>*@
                    <select id="ddlCompany" class="select2-select-00"
                            ng-model="lstCompanyList"
                            ng-change="LoadBankAdvisingByCompanyID()"
                            name="ddlCompany"
                            data-placeholder="--Select Company--"
                            style="width: 100%" ng-disabled="!bool"
                            ng-options="com.CompanyID as com.CompanyName for com in listCompany" required>
                        <option></option>
                    </select>

                </div>
                <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlCompany.$error.required">*</span>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label" for="input17">Buyer </label>
                <div class="col-md-6">
                    @*<p>Selected Buyer: {{lstBuyerList}}</p>*@
                    <select id="ddlBuyer" class="select2-select-00"
                            ng-model="lstBuyerList" ng-change="LoadBookingNoByBuyerID()"
                            name="ddlBuyer"
                            data-placeholder="--Select Buyer--"
                            style="width: 100%" ng-options="buy.UserID as buy.UserFullName for buy in listBuyer" required>
                        <option></option>
                        @*<option ng-repeat="buy in listBuyer" value="{{buy.UserID}}">{{buy.UserFullName}}</option>*@
                    </select>
                </div>
                <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlBuyer.$error.required">*</span>
            </div>
            <div class="form-group">
                <label class="col-md-4 control-label" for="input17">Booking No </label>
                <div class="col-md-6">
                    <select id="ddlBookingList" class="select2-select-00"
                            ng-model="lstBookingNG"
                            name="ddlBookingList" ng-change="LoadDetailByBookingID()"
                            data-placeholder="--Select Booking--"
                            style="width: 100%" ng-options="vl.BookingID as vl.BookingNo for vl in listBooking" required>
                        <option></option>
                    </select>
                </div>
                <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlBookingList.$error.required">*</span>
            </div>
            

            <div class="form-group">
                <label class="col-md-4 control-label">PI NO </label>
                <div class="col-md-6">
                    <input type="text"
                           id="pINo"
                           class="form-control" style="font-weight:bold; color:red !important;"
                           name="pINo" ng-model="HPINO" placeholder="New" ng-readonly="true" /> @*//     validation problem*@
                    </div>
                    @*<span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.pINo.$error.required">*</span>*@
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label">PI Date </label>
                    <div class="col-md-3">
                        <input type="text"
                               id="dtpIDate"
                               class="form-control datepicker"
                               name="dtpIDate" ng-model="PIDate" @*placeholder="dd-MM-yyyy"*@ @*value="{{PIDate | date:'dd-MM-yyyy'}}"*@ required ng-disabled="true" readonly="readonly" />
                    </div>
                    <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.dtpIDate.$error.required">*</span>
                </div>


                <div class="form-group">
                    <label class="col-md-4 control-label" for="input17">Bank Advising </label>
                    <div class="col-md-6">
                        <select id="ddlBankAdvising" class="select2-select-00"
                                ng-model="lstBankAdvisingList"
                                name="ddlBankAdvising"
                                data-placeholder="--Select Bank Advising--"
                                style="width: 100%" ng-change="LoadBranchByBankID()" ng-options="bnk.BankID as bnk.BankName for bnk in listBankAdvising" required>

                            <option></option>
                            @*<option ng-repeat="bnk in listBankAdvising" value="{{bnk.BankID}}">{{bnk.BankName}}</option>*@


                        </select>
                    </div>
                    <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlBankAdvising.$error.required">*</span>
                </div>

                <div class="form-group">
                    <label class="col-md-4 control-label" for="input17">Bank Branch </label>
                    <div class="col-md-6">
                        <select id="ddlBankBranch" class="select2-select-00"
                                ng-model="lstBankBranchList"
                                name="ddlBankBranch"
                                data-placeholder="--Select Bank Branch--"
                                style="width: 100%" ng-options="bnkBranch.BranchID as bnkBranch.BranchName for bnkBranch in listBankBranch" required>
                            <option></option>
                            @*<option ng-repeat="bnkBranch in listBankBranch" value="{{bnkBranch.BranchID}}">{{bnkBranch.BranchName}}</option>*@
                        </select>
                    </div>
                    <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlBankBranch.$error.required">*</span>
                </div>



