                                    name="ddlShipment"
                                    data-placeholder="--Select Shipment--"
                                    style="width: 100%" ng-options="spt.ComboID as spt.ComboName for spt in listShipment" required>
                                <option></option>
                                @*<option ng-repeat="spt in listShipment" value="{{spt.ComboID}}">{{spt.ComboName}}</option>*@
                            </select>
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlShipment.$error.required">*</span>
