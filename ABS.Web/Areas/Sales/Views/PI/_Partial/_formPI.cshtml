                        <label class="col-md-4 control-label" for="input17">Validity </label>
                        <div class="col-md-4">
                            <select id="ddlValidity" class="select2-select-00"
                                    ng-model="lstValidityList"
                                    name="ddlValidity"
                                    data-placeholder="--Select Validity--"
                                    style="width: 100%" ng-options="vl.ComboID as vl.ComboName for vl in listValidity" required>
                                <option></option>
                                @*<option ng-repeat="vl in listValidity" value="{{vl.ComboID}}">{{vl.ComboName}}</option>*@
                            </select>
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlValidity.$error.required">*</span>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="input17">Acceptable Qty(%) </label>
                        <div class="col-md-4">
