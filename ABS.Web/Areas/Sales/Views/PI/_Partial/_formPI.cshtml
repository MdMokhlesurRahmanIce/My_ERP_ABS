                                    data-placeholder="--Select Acceptable Quantity--"
                                    style="width: 100%" ng-options="vl.ComboID as vl.ComboName for vl in listAcceptableQuantity" required>
                                <option></option>
                                @*<option ng-repeat="vl in listValidity" value="{{vl.ComboID}}">{{vl.ComboName}}</option>*@
                            </select>
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlValidity.$error.required">*</span>
                    </div>

                    <div class="form-group">
                        <label class="col-md-4 control-label" for="input17">Sight(Days) </label>
                        <div class="col-md-4">
                            <select id="ddlSight" class="select2-select-00"
                                    ng-model="lstSightList"
                                    name="ddlSight"
                                    data-placeholder="--Select Sight--"
                                    style="width: 100%" ng-options="sgt.ComboID as sgt.ComboName for sgt in listSight" required>
                                <option></option>
                                @*<option ng-repeat="sgt in listSight" value="{{sgt.ComboID}}">{{sgt.ComboName}}</option>*@
                            </select>
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlSight.$error.required">*</span>
                    </div>

                    <div class="form-group">
                        <label class="col-md-4 control-label">Negotiation </label>
                        <div class="col-md-3">
                            <input type="number"
                                   id="txtNegotiation"
                                   class="form-control"
                                   name="txtNegotiation" ng-model="Negotiation" min="0" max="60" required />
                        </div>
                        <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.txtNegotiation.$error.required">*</span>
                    </div>


                    @*<div class="form-group">
                            <label class="col-md-4 control-label">Discount </label>
