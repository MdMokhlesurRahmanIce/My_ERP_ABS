                <div class="form-group">
                    <label class="col-md-4 control-label" for="input17">Incoterm </label>
                    <div class="col-md-4">
                        <select id="ddlIncoterm" class="select2-select-00"
                                ng-model="lstIncoNG"
                                name="ddlIncoterm"
                                data-placeholder="--Select Inco.--"
                                style="width: 100%" ng-options="vl.IncoTermID as vl.IncotermName for vl in listIncoterm" required>
                            <option></option>
                        </select>
                    </div>
                    <span class="error" ng-show="(frmPI.$dirty||submitted) && frmPI.ddlValidity.$error.required">*</span>
                </div>
                <div class="form-group">
                    <label class="col-md-4 control-label">Destination </label>
                    <div class="col-md-6">
                        <input type="text"
