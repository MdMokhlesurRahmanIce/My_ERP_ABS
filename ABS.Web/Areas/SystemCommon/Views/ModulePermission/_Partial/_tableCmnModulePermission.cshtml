

<table class="table  table-hover table-striped">
    <thead class="grid-top-panel">
        <tr>
            <th style="display:none">ModulePermissionID</th>
            <th>SL.</th>
            <th>Custom Code</th>
            <th>Module</th>
            <th>Company</th>
            <th style="text-align:right;">Options</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataModel in ListModulePermission |filter:UserSearch">
            <td style="display:none">{{dataModel.ModulePermissionID}}</td>
            <td>{{$index+1}}</td>
            <td>{{dataModel.CustomCode}}</td>
            <td>{{dataModel.ModuleName}}</td>
            <td>{{dataModel.CompanyName}}</td>
            <td style="text-align:right; color:white">
                <span class="label label-info label-mini">
                    <i class="{{dataModel.IsActive}}"></i>
                </span>
                @*Edit Is Not Implemented Do It Later*@
                @*<span class="label label-warning label-mini">
                        <a ng-href="#userModal" data-toggle="modal" class="bs-tooltip" title="Edit Info">
                            <i class="icon-pencil" ng-click="getUser(dataModel)"></i>
                        </a>
                    </span>*@
                <span class="label label-danger label-mini">
                    <a href="javascript:void(0);" class="bs-tooltip" title="Delete" ng-click="delete(dataModel)">
                        <i class="icon-trash"></i>
                    </a>
                </span>
            </td>
        </tr>
    </tbody>
</table>
<p ng-show="(listModulePermission | filter:UserSearch).length == 0" style="color:red">Nothing found!</p>

<div class="loadmore">
    <div ng-show="loaderMore" ng-class="result">
        <img src="~/Content/img/ng-loader.gif" />
        {{lblMessage}}
    </div>
    <div ng-show="scrollended" ng-class="result">
        {{lblMessage}}
    </div>
</div>